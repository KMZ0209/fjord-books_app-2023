<p style="color: green"><%= notice %></p>

<h1><%= t('activerecord.views.common.index') %></h1>

<div id="books" class="index-items">
  <% @books.each do |book| %>
  <div class="index-item">
    <%= render book %>
    <p>
      <%= link_to t('activerecord.button.show'), book %>
    </p>
  </div>
  <% end %>
</div>
<nav class="page-nav">
<%= link_to t('activerecord.button.newbook'), new_book_path %>
</nav>
